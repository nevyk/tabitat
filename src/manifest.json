{
  "name": "Tabitat",
  "description": "A habitat for tabs",
  "version": "0.1.0",
  "version_name": "0.1.0",
  "manifest_version": 2,
  "icons": {
    "32": "icons/Tabitat_32x32.png",
    "64": "icons/Tabitat_64x64.png",
    "96": "icons/Tabitat_96x96.png",
    "128": "icons/Tabitat_128x128.png",
    "256": "icons/Tabitat_256x256.png",
    "512": "icons/Tabitat_512x512.png"
  },
  "browser_action": {
    "default_title": "Tabitat",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Period"
      }
    }
  }
}